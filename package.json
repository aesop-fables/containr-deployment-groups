{
  "name": "@aesop-fables/containr-deployment-groups",
  "version": "0.1.0",
  "description": "Provides structured/cascading access to ambient and tenant-agnostic settings",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rimraf ./lib && tsc --p ./tsconfig.build.json",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "esbuild": "^0.17.11",
    "eslint": "8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash": "4.17.21",
    "jest": "29.5.0",
    "prettier": "^2.8.1",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@aesop-fables/containr": "^0.2.3",
    "@aesop-fables/containr-dynamofx": "^0.1.4",
    "jest-mock-extended": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "@aws-sdk/client-dynamodb": "^3.267.0",
    "@aws-sdk/lib-dynamodb": "^3.350.0",
    "@types/aws-lambda": "^8.10.110",
    "@aws-sdk/smithy-client": "^3.0.0",
    "@aws-sdk/types": "^3.0.0",
    "aws-sdk": "^2.1343.0"
  }
}
